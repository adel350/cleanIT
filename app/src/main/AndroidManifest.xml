<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cleanit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".OrderDetailsOne"></activity>
        <activity android:name=".UpdateInfoScreen" />
        <activity android:name=".BeforeFinalizingScreen" />
        <activity android:name=".NewOrderConfirmation" />
        <activity android:name=".OrderDetails" />
        <activity android:name=".AccountDetails" />
        <activity android:name=".PaymentInformation" />
        <activity android:name=".AdditionalSignUpStage" />
        <activity android:name=".SignIn" />
        <activity android:name=".EmailSignUp" />
        <activity android:name=".FacebookSignUp" />
        <activity android:name=".GoogleSignUp" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>